{
  "name": "kreyol",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node ./app/index.js",
    "build": "cd frontend && npm install && npm run build",
    "dev:server": "nodemon ./app/index.js | pino-pretty",
    "dev": "concurrently 'npm run dev:server' 'cd frontend && HTTPS=true npm run start' ",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "pwd": "node ./app/scripts/hash.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://git.heroku.com/kreyol.git"
  },
  "nodemonConfig": {
    "ignore": ["frontend/*", "db/*", "old/*"]
  },  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.76.0",
    "argon2": "^0.28.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.17.2",
    "express-session-sequelize": "^2.3.0",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "node-cache": "^5.1.2",
    "nspell": "^2.1.5",
    "passport": "^0.5.2",
    "passport-jwt-cookiecombo": "^1.0.5",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pino": "^7.10.0",
    "sequelize": "^6.19.0",
    "sha3": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.6.1",
    "sequelize-cli": "^6.4.1"
  }
}
