{
  "name": "react-zakari",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/react-zakari/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/react-zakari",
        "main": "libs/react-zakari/src/index.js",
        "tsConfig": "libs/react-zakari/tsconfig.lib.json",
        "assets": [],
        "webpackConfig": "libs/custom-webpack.config.js"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/react-zakari/**/*.js"]
      }
    }
  },
  "tags": []
}
